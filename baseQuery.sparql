  		BIND ("en" as ?l).
      	?entry a schema:DefinedTerm.
      
      	# Abkürzung
      	OPTIONAL {
        	?entry schema:alternateName ?ENG_Abkürzung_1. FILTER(lang(?ENG_Abkürzung_1) = ?l)
        	?entry schema:hasPart ?alternateNamePart.
        	?alternateNamePart a aschema:Abbreviation.
        	#?alternateNamePart schema:name ?ENG_Abkürzung_1. FILTER(lang(?ENG_Abkürzung_1) = ?l). # To exclude Synonyms.
            FILTER(STRENDS(str(?alternateNamePart),"abbreviation"))
            ?alternateNamePart schema:citation ?ENG_Abkürzung_Quelle_1. FILTER(lang(?ENG_Abkürzung_Quelle_1) = ?l).
      	}
      
      	# Land
      	OPTIONAL {
        	?entry aschema:applicableCountry ?ENG_Land_1. FILTER(lang(?ENG_Land_1) = ?l)
        }
      
      	# Definition
