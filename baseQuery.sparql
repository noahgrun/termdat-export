FROM <https://lindas.admin.ch/fch/termdat> {

  # Sammlung
  VALUES ?collection {termdat:DEC19}
  
  # Entry, TERMDAT-Link
  {
    ?entry a schema:DefinedTerm.
  	?entry schema:inDefinedTermSet ?collection .
    BIND (?entry as ?Header_TERMDAT_Link).
	BIND (iri(CONCAT(str(?entry),"?disableSchemaUrlRedirect=true")) as ?LINDAS_Link).  #TODO remove
  }
  
  # Terminologiesammlung
  {
    ?collection a aschema:TerminologyCollection;
 	schema:name ?collectionName;
  	schema:identifier ?collectionId. FILTER (lang(?collectionName) = "de").
  	BIND (CONCAT(?collectionId, " - ", ?collectionName) as ?Header_Terminologiesammlung).
  }
  
  # Fachgebiet
  {
    SELECT ?entry (GROUP_CONCAT(?domainName; separator=", ") AS ?Header_Fachgebiet)  WHERE {
  		?entry aschema:termDomain ?domain.
    	?domain schema:name ?domainName. FILTER(lang(?domainName) = "de")
  	} GROUP BY ?entry
  }
  
  
### GERMAN
  {
    SELECT 
      ?entry

	  ?DES_Name_1
	  ?DES_Name_Quelle_1
	  ?DES_Name_2
	  ?DES_Name_Quelle_2
	  ?DES_Name_3
	  ?DES_Name_Quelle_3    
    
	  ?DES_Terminus_1
	  ?DES_Terminus_Quelle_1
	  ?DES_Terminus_2
	  ?DES_Terminus_Quelle_2    
	  ?DES_Terminus_3
	  ?DES_Terminus_Quelle_3    
    
	  ?DES_Phraseologie_1
	  ?DES_Phraseologie_Quelle_1
	  ?DES_Phraseologie_2
	  ?DES_Phraseologie_Quelle_2
	  ?DES_Phraseologie_3
	  ?DES_Phraseologie_Quelle_3
    
	  ?DES_Definition_1
	  ?DES_Definition_Quelle_1
	  ?DES_Definition_2
	  ?DES_Definition_Quelle_2
	  ?DES_Definition_3
	  ?DES_Definition_Quelle_3
    
      ?DES_Abkürzung_1
      ?DES_Abkürzung_Quelle_1

    
      ?DES_Anmerkung_1
      ?DES_Anmerkung_Quelle_1
      ?DES_Anmerkung_2
      ?DES_Anmerkung_Quelle_2
      ?DES_Anmerkung_3
      ?DES_Anmerkung_Quelle_3

      ?DES_Land_1
      ?DES_Land_2
      ?DES_Land_3    
    WHERE {
  		BIND ("de" as ?l).
      	?entry a schema:DefinedTerm.
      
      	# Abkürzung
      	OPTIONAL {
        	?entry schema:alternateName ?DES_Abkürzung_1. FILTER(lang(?DES_Abkürzung_1) = ?l)
        	?entry schema:hasPart ?alternateNamePart.
        	?alternateNamePart a aschema:Abbreviation.
        	#?alternateNamePart schema:name ?DES_Abkürzung_1. FILTER(lang(?DES_Abkürzung_1) = ?l). # To exclude Synonyms.
            FILTER(STRENDS(str(?alternateNamePart),"abbreviation"))
            ?alternateNamePart schema:citation ?DES_Abkürzung_Quelle_1. FILTER(lang(?DES_Abkürzung_Quelle_1) = ?l).
      	}
      
      	# Land
      	OPTIONAL {
        	?entry aschema:applicableCountry ?DES_Land_1. FILTER(lang(?DES_Land_1) = ?l)
        }
      
      	# Definition
      	OPTIONAL {
        	?entry schema:description ?DES_Definition_1. FILTER(lang(?DES_Definition_1) = ?l)
     
           	?entry aschema:descriptionCitation ?DES_Definition_Quelle_1. FILTER(lang(?DES_Definition_Quelle_1) = ?l)
        }
      
       	# Anmerkung
      	OPTIONAL {
        	?entry aschema:termNote ?DES_Anmerkung_1. FILTER(lang(?DES_Anmerkung_1) = ?l)
        
        }     
	    # Anmerkung Quelle
      	OPTIONAL {
          		?entry aschema:termNoteCitation ?DES_Anmerkung_Quelle_1. FILTER(lang(?DES_Anmerkung_Quelle_1) = ?l)
      	}
      
      	# Name
      	OPTIONAL {
            ?entry a aschema:Name.
        	?entry schema:name ?DES_Name_1. FILTER(lang(?DES_Name_1) = ?l)
          	?entry schema:citation ?DES_Name_Quelle_1. FILTER(lang(?DES_Name_Quelle_1) = ?l)

      	}
      
      	OPTIONAL { # Wenn in der Type in der Sprache anders als Haupteintrag
        	?entry schema:hasPart ?namePart.
        	?namePart a aschema:Name.
            FILTER(STRENDS(str(?phraseologyPart),"name"))         
        	?namePart schema:name ?DES_Name_1. FILTER(lang(?DES_Name_1) = ?l)
          	?namePart schema:citation ?DES_Name_Quelle_1. FILTER(lang(?DES_Name_Quelle_1) = ?l)
       
      	}
      
      	# Term
      	OPTIONAL {
            ?entry a aschema:Term.
        	?entry schema:name ?DES_Terminus_1. FILTER(lang(?DES_Terminus_1) = ?l)
        
          	?entry schema:citation ?DES_Terminus_Quelle_1. FILTER(lang(?DES_Terminus_Quelle_1) = ?l)
       
      	}
      	OPTIONAL { # Wenn in der Type in der Sprache anders als Haupteintrag
        	?entry schema:hasPart ?termPart.
        	?termPart a aschema:Term.
            FILTER(STRENDS(str(?phraseologyPart),"term"))                
        	?termPart schema:name ?DES_Terminus_1. FILTER(lang(?DES_Terminus_1) = ?l)
        	?termPart schema:citation ?DES_Terminus_Quelle_1. FILTER(lang(?DES_Terminus_Quelle_1) = ?l)
        	          
      	}
      
      	# Phraseology
      	OPTIONAL {
            ?entry a aschema:Phraseology.
        	?entry schema:name ?DES_Phraseologie_1. FILTER(lang(?DES_Phraseologie_1) = ?l)
   
          	?entry schema:citation ?DES_Phraseologie_Quelle_1. FILTER(lang(?DES_Phraseologie_Quelle_1) = ?l)
        	          
      	}
      	OPTIONAL { # Wenn in der Type in der Sprache anders als Haupteintrag
        	?entry schema:hasPart ?phraseologyPart.
        	?phraseologyPart a aschema:Phraseology.
            FILTER(STRENDS(str(?phraseologyPart),"phraseology"))        
        	?phraseologyPart schema:name ?DES_Phraseologie_1. FILTER(lang(?DES_Phraseologie_1) = ?l)
          	?phraseologyPart schema:citation ?DES_Phraseologie_Quelle_1. FILTER(lang(?DES_Phraseologie_Quelle_1) = ?l)
      	}
      
      
      	# Land.2
      	OPTIONAL {
            ?entry schema:hasPart ?landPart2.
            FILTER(STRENDS(str(?landPart2),concat("/2/",?l)))        
        	?landPart2 aschema:applicableCountry ?DES_Land_2. FILTER(lang(?DES_Land_2) = ?l).
        }
        # Land.3
      	OPTIONAL {
            ?entry schema:hasPart ?landPart3.
            FILTER(STRENDS(str(?landPart3),concat("/3/",?l)))        
        	?landPart3 aschema:applicableCountry ?DES_Land_3. FILTER(lang(?DES_Land_3) = ?l).
        }

      	# Definition.2
      	OPTIONAL {
        	?entry schema:hasPart ?definitionPart2.
            FILTER(STRENDS(str(?definitionPart2),concat("/2/",?l)))        
        	?definitionPart2 schema:description ?DES_Definition_2. FILTER(lang(?DES_Definition_2) = ?l)
           	?definitionPart2 aschema:descriptionCitation ?DES_Definition_Quelle_2. FILTER(lang(?DES_Definition_Quelle_2) = ?l)
        }      
      	# Definition.3
      	OPTIONAL {
        	?entry schema:hasPart ?definitionPart3.
            FILTER(STRENDS(str(?definitionPart3),concat("/3/",?l)))        
        	?definitionPart3 schema:description ?DES_Definition_3. FILTER(lang(?DES_Definition_3) = ?l)
           	?definitionPart3 aschema:descriptionCitation ?DES_Definition_Quelle_3. FILTER(lang(?DES_Definition_Quelle_3) = ?l)
        }    

      	# Anmerkung.2
      	OPTIONAL {
        	?entry schema:hasPart ?termNotePart2.
            FILTER(STRENDS(str(?termNotePart2),concat("/2/",?l)))        
        	?termNotePart2 aschema:termNote ?DES_Anmerkung_2. FILTER(lang(?DES_Anmerkung_2) = ?l)
        }
      	# Anmerkung.2 Quelle
      	OPTIONAL {
        	?entry schema:hasPart ?termNoteCitationPart2.
            FILTER(STRENDS(str(?termNoteCitationPart2),concat("/2/",?l)))        
        	?termNoteCitationPart2 aschema:termNoteCitation ?DES_Anmerkung_Quelle_2. FILTER(lang(?DES_Anmerkung_Quelle_2) = ?l)
        }    
      	# Anmerkung.3
      	OPTIONAL {
        	?entry schema:hasPart ?termNotePart3.
            FILTER(STRENDS(str(?termNotePart3),concat("/3/",?l)))        
        	?termNotePart3 aschema:termNote ?DES_Anmerkung_3. FILTER(lang(?DES_Anmerkung_3) = ?l)
        }
      	# Anmerkung.3 Quelle
      	OPTIONAL {
        	?entry schema:hasPart ?termNoteCitationPart3.
            FILTER(STRENDS(str(?termNoteCitationPart3),concat("/3/",?l)))        
        	?termNoteCitationPart3 aschema:termNoteCitation ?DES_Anmerkung_Quelle_3. FILTER(lang(?DES_Anmerkung_Quelle_3) = ?l)
        }        
      
        # Name.2
      	OPTIONAL {
        	?entry schema:hasPart ?namePart2.
            FILTER(STRENDS(str(?namePart2),concat("/2/",?l)))        
        	?namePart2 a aschema:Name.
        	?namePart2 schema:name ?DES_Name_2. FILTER(lang(?DES_Name_2) = ?l)
          	?namePart2 schema:citation ?DES_Name_Quelle_2. FILTER(lang(?DES_Name_Quelle_2) = ?l)
      	}
        # Name.3
      	OPTIONAL {
        	?entry schema:hasPart ?namePart3.
            FILTER(STRENDS(str(?namePart3),concat("/3/",?l)))        
        	?namePart3 a aschema:Name.
        	?namePart3 schema:name ?DES_Name_3. FILTER(lang(?DES_Name_3) = ?l)
          	?namePart3 schema:citation ?DES_Name_Quelle_3. FILTER(lang(?DES_Name_Quelle_3) = ?l)
      	}
      
      	# Term.2
      	OPTIONAL {
        	?entry schema:hasPart ?termPart2.
            FILTER(STRENDS(str(?termPart2),concat("/2/",?l)))
        	?termPart2 a aschema:Term.
        	?termPart2 schema:name ?DES_Terminus_2. FILTER(lang(?DES_Terminus_2) = ?l)
        	?termPart2 schema:citation ?DES_Terminus_Quelle_2. FILTER(lang(?DES_Terminus_Quelle_2) = ?l)
      	}
      	# Term.3
      	OPTIONAL {
        	?entry schema:hasPart ?termPart3.
            FILTER(STRENDS(str(?termPart3),concat("/3/",?l)))
        	?termPart3 a aschema:Term.
        	?termPart3 schema:name ?DES_Terminus_3. FILTER(lang(?DES_Terminus_3) = ?l)
        	?termPart3 schema:citation ?DES_Terminus_Quelle_3. FILTER(lang(?DES_Terminus_Quelle_3) = ?l)
      	}
      
      	# Phraseology.2      
        OPTIONAL {
        	?entry schema:hasPart ?phraseologyPart2.
            FILTER(STRENDS(str(?phraseologyPart2),concat("/2/",?l)))
        	?phraseologyPart2 a aschema:Phraseology.
        	?phraseologyPart2 schema:name ?DES_Phraseologie_2. FILTER(lang(?DES_Phraseologie_2) = ?l)
          	?phraseologyPart2 schema:citation ?DES_Phraseologie_Quelle_2. FILTER(lang(?DES_Phraseologie_Quelle_2) = ?l)
      	}
      	# Phraseology.3     
        OPTIONAL {
        	?entry schema:hasPart ?phraseologyPart3.
            FILTER(STRENDS(str(?phraseologyPart3),concat("/3/",?l)))
        	?phraseologyPart3 a aschema:Phraseology.
        	?phraseologyPart3 schema:name ?DES_Phraseologie_3. FILTER(lang(?DES_Phraseologie_3) = ?l)
          	?phraseologyPart3 schema:citation ?DES_Phraseologie_Quelle_3. FILTER(lang(?DES_Phraseologie_Quelle_3) = ?l)
      	}      
      
   	}
  }
	
  
